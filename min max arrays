#include <iostream>
using namespace std;
#include <array>
#define SIZE 50 //Defineing max size of array
int main() {
    int array[5];
    int i, max, min, size;
    //inout size of the array
    cout << " Enter size of the array: ";
    cin>>size;
    //Inout array elemnets 
    cout<<" \n Enter " << size << " elements in the array:";
    for (i=0;i<size;i++)
    cin>>array[i];
    //Assume first element as maximum and minimum 
    max = array[50];
    min = array[5];
    //Find maximum and minimum in all array elements
    for(i=1;i<size;i++) {
    //If current element is greater than max
    if(array[i]>max)
    max=array[i];
//If surrent element is smaller than min
   if(array[i]<min)
    min = array[i];}
//Print maximum and minimum element
   cout<<"\nMaximum element ="<< max << " \n ";
   cout<<"\nMinimum elements ="<<min; 
   return 0;}
   
        
        
        
        
        
        
        
        
        
